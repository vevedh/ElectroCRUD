{
  "productName": "ElectroCRUD",
  "directories": {
    "output": "release/"
  },
    "files": [
        "**/*",
        "src/shared/**/*.js",
        "src/backend/**/*.js",
        "!**/*.ts",
        "!*.code-workspace",
        "!LICENSE.md",
        "!package.json",
        "!package-lock.json",
        "!src/app",
        "!src/*.ts",
        "!src/*.html",
        "!src/*.ico",
        "!src/*.png",
        "!src/*.icns",
        "!src/*.js",
        "!src/*.scss",
        "!src/*.json",
        "!e2e/",
        "!hooks/",
        "!angular.json",
        "!_config.yml",
        "!karma.conf.js",
        "!tsconfig.json",
        "!tslint.json"
    ],
  "win": {
    "icon": "dist",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}
